  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "unitOfMeasure": {
      "type": "string",
      "required": true
    },
    "unitOfMeasureConversion": {
      "type": "string",
      "required": true
    },
    "quantityConversion": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "rations": {
      "type": "hasMany",
      "model": "ration",
      "foreignKey": "productId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
